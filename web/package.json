{
  "name": "@trinsic/trinsic",
  "version": "1.0.0",
  "description": "Node and Browser wrapper for the Trinsic services",
  "browser": {
    "lib/node.js": "lib/browser.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
        "browser": [
            "./lib/browser/index.d.ts"
        ],
        "node": [
            "./lib/node/index.d.ts"
        ]
    }
  },
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./browser": "./lib/browser/index.js",
    "./node": "./lib/node/index.js"
  },
  "scripts": {
    "build": "npm run build:proto && npm run build:typescript",
    "build:proto": "pwsh Generate-Proto.ps1",
    "build:typescript": "npx tsc",
    "test:node": "jest --config .config/jest.config.cjs --no-cache",
    "test:browser": "karma start .config/karma.conf.cjs",
    "test": "npm run test:node && npm run test:browser",
    "build-and-test": "npm run build && npx run test"
  },
  "author": "Trinsic",
  "contributors": [
    {
      "name": "Michael Black",
      "email": "michaelblack117@gmail.com"
    },
    {
      "name": "Tomislav Markovski",
      "email": "tmarkovski@gmail.com"
    },
    {
      "name": "Scott Phillips",
      "email": "polygonguru@gmail.com"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@improbable-eng/grpc-web-node-http-transport": "0.15.0",
    "@trinsic/okapi-node": "1.6.1-rc3",
    "@trinsic/okapi-web": "1.6.1-rc3",
    "buffer": "6.0.3",
    "google-protobuf": "3.21.0",
    "long": "5.2.0",
    "nice-grpc": "2.0.0",
    "nice-grpc-web": "2.0.0",
    "js-base64": "3.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/plugin-transform-modules-commonjs": "7.18.6",
    "@types/google-protobuf": "3.15.6",
    "@types/jasmine": "4.0.3",
    "@types/jest": "29.0.0",
    "@types/karma": "6.3.3",
    "@types/karma-webpack": "2.0.11",
    "@types/node": "18.7.8",
    "@types/uuid": "8.3.4",
    "babel-jest": "28.1.3",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chromedriver": "104.0.0",
    "copyfiles": "2.4.1",
    "dotenv": "16.0.1",
    "esm": "3.2.25",
    "expect": "29.0.1",
    "jasmine": "4.3.0",
    "jasmine-core": "4.3.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "29.0.1",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "karma-sourcemap-loader": "0.3.8",
    "karma-typescript": "5.5.3",
    "karma-webpack": "5.0.0",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "ts-proto": "1.122.0",
    "typescript": "4.7.4",
    "uuid": "8.3.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  }
}
